	"dependencies": {
		"@popperjs/core": "^2.11.6",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^13.4.0",
		"@testing-library/user-event": "^14.4.3",
		"@types/jest": "^29.2.5",
		"@types/node": "^18.11.18",
		"@types/prop-types": "^15.7.3",
		"@types/react": "^18.0.26",
		"@types/react-dom": "^18.0.10",
		"@types/react-router-dom": "^5.1.5",
		"animate.css": "^4.1.0",
