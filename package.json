		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"eslint": "^8.31.0",
		"eslint-plugin-react": "^7.20.3",
		"eslint-plugin-react-hooks": "^4.0.8",
		"gh-pages": "^4.0.0",
		"typescript": "^4.9.4",
		"yargs-parser": "^21.1.1"
	}
